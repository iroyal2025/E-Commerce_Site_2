      "source": [
        "\n",
        "# Assuming X and Y are your features and labels\n",
        "\n",
        "# Convert string labels to integers\n",
        "label_encoder = LabelEncoder()\n",
        "\n",
        "# If Y contains strings, encode them as integers\n",
        "Y_encoded = label_encoder.fit_transform(Y)\n",
        "\n",
        "# Check the unique values in y_train and y_test before one-hot encoding\n",
        "print(\"Unique values in Y_encoded before one-hot encoding:\", set(Y_encoded))\n",
        "\n",
        "# Split data into training and test sets\n",
        "X_train, X_test, y_train, y_test = train_test_split(X, Y_encoded, test_size=0.20, random_state=42)\n",
        "\n",
        "# Dynamically calculate the number of classes\n",
        "num_classes = len(set(y_train))  # Use the unique values in y_train\n",
        "\n",
        "# Convert labels to one-hot encoding (binary or multi-class)\n",
        "y_train = to_categorical(y_train, num_classes=num_classes)\n",
        "y_test = to_categorical(y_test, num_classes=num_classes)\n",
        "\n",
        "# Check the shape of y_train and y_test after one-hot encoding\n",
        "print(\"Shape of y_train after one-hot encoding:\", y_train.shape)\n",
        "print(\"Shape of y_test after one-hot encoding:\", y_test.shape)\n",
        "\n",
        "# Build the model\n",
        "model = Sequential()\n",
        "\n",
        "# Embedding layer (input_dim = 500, output_dim = 120)\n",
        "model.add(Embedding(input_dim=500, output_dim=120, input_length=X_train.shape[1]))\n",
        "\n",
        "# SpatialDropout1D\n",
        "model.add(SpatialDropout1D(0.4))\n",
        "\n",
        "# LSTM layer\n",
        "model.add(LSTM(176, dropout=0.2, recurrent_dropout=0.2))\n",
        "\n",
        "# Dense output layer with softmax activation for multi-class classification\n",
        "model.add(Dense(num_classes, activation='softmax'))  # num_classes is dynamically set\n",
        "\n",
        "# Compile the model\n",
        "model.compile(loss='categorical_crossentropy',\n",
        "              optimizer='adam',\n",
        "              metrics=['accuracy'])\n",
        "\n",
        "# Model summary\n",
        "model.summary()\n",
        "\n",
        "# Train the model\n",
        "model.fit(X_train, y_train,\n",
        "          epochs=5,\n",
        "          batch_size=32,\n",
        "          verbose=1,\n",
        "          validation_data=(X_test, y_test))"
