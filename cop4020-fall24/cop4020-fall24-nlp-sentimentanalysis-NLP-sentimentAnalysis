        "stopwords_list = set(stopwords.words('english'))\n",
        "data['verified reviews'] = data['verified reviews'].apply(lambda word:' '.join([word for word in word.split() if word not in stopwords_list]))\n",
        "\n",
        "#printing dataset\n",
        "data.head(20)"
