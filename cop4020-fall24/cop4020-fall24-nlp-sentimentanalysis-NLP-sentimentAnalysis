      "cell_type": "code",
      "source": [
        "#Cleaning the dataset by removing special characters and getting rid of uppercase charatcers.\n",
        "\n",
        "\n",
        "data['verified reviews'] = data['verified reviews'].apply(\n",
        "    lambda word: ' '.join(word.lower() for word in str(word).split()) if isinstance(word, str) else ''\n",
        ")\n",
        "#\n",
        "data['verified reviews'] = data['verified reviews'].str.replace('\\d+',' ',regex=True)\n",
